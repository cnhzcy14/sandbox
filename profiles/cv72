[settings]
os=Linux
arch=armv8
compiler=gcc
compiler.cppstd=gnu17
compiler.libcxx=libstdc++11
compiler.version=12
build_type=Release
[buildenv]
CC=aarch64-linux-gnu-gcc-12
CXX=aarch64-linux-gnu-g++-12
LD=aarch64-linux-gnu-ld
CFLAGS=-Os -march=armv8-a+crypto+crc+fp+simd -mtune=cortex-a72 -ffunction-sections -fdata-sections
CXXFLAGS=-Os -march=armv8-a+crypto+crc+fp+simd -mtune=cortex-a72 -ffunction-sections -fdata-sections
LDFLAGS=-Wl,--gc-sections
