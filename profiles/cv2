[settings]
os=Linux
arch=armv8
compiler=gcc
compiler.cppstd=gnu17
compiler.libcxx=libstdc++11
compiler.version=8
build_type=Release
[buildenv]
CC=aarch64-linux-gnu-gcc-8
CXX=aarch64-linux-gnu-g++-8
LD=aarch64-linux-gnu-ld
CFLAGS=-Os -march=armv8-a+crypto -mtune=cortex-a53 -ffunction-sections -fdata-sections
CXXFLAGS=-Os -march=armv8-a+crypto -mtune=cortex-a53 -ffunction-sections -fdata-sections
LDFLAGS=-Wl,--gc-sections
