[settings]
os=Linux
arch=armv8
compiler=gcc
compiler.cppstd=gnu17
compiler.libcxx=libstdc++11
compiler.version=9
build_type=Release

[buildenv]
CC=aarch64-linux-gnu-gcc-9
CXX=aarch64-linux-gnu-g++-9
LD=aarch64-linux-gnu-ld
AR=aarch64-linux-gnu-ar
RANLIB=aarch64-linux-gnu-ranlib
STRIP=aarch64-linux-gnu-strip

CFLAGS=-Os -march=armv8.2-a+crypto -mtune=cortex-a55 -ffunction-sections -fdata-sections
CXXFLAGS=-Os -march=armv8.2-a+crypto -mtune=cortex-a55 -ffunction-sections -fdata-sections
LDFLAGS=-Wl,--gc-sections,--strip-all
